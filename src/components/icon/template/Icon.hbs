<i
class="${prefix}icon
{{#if name}} ${prefix}icon-{{type}}-{{name}}{{/if}}
{{#if spin}} ${prefix}icon-spin{{/if}}
{{#if className}} {{className}}{{/if}}"

{{#if size > 0 || color || style}}
  style="
  {{#if size > 0}}font-size: {{size}}px;{{/if}}
  {{#if color}}color: {{color}};{{/if}}
  {{#if style}}{{style}}{{/if}}"
{{/if}}
>
  <slot name="children" />
</i>