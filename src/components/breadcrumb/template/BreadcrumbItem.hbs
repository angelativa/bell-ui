<span class="${prefix}breadcrumb-item ${prefix}text
{{#if className}} {{className}}{{/if}}"

{{#if style}} style="{{style}}"{{/if}}
>

  <a class="${prefix}breadcrumb-link"
    {{#if to}} href="{{to}}"{{/if}}
  >
    <slot name="children" />
  </a>

  <span class="${prefix}breadcrumb-gap">
    {{separator}}
  </span>

</span>